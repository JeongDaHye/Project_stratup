<!DOCTYPE html>
<html lang="en">

<head>
  <!-- meta -->
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">

  <title>programmer</title>
  <meta content="" name="keywords">
  <meta content="" name="description">

  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css?family=Poppins:300,300i,400,400i,500,500i,600,600i,700,700i|Playfair+Display:400,400i,700,700i,900,900i" rel="stylesheet">

  <!-- Bootstrap CSS File -->
  <link href="assets/bootstrap.min.css" rel="stylesheet">

  <!-- Libraries CSS Files -->
  <link href="assets/ionicons.min.css" rel="stylesheet">
  <link href="assets/owl.carousel.min.css" rel="stylesheet">
  <link href="assets/magnific-poApup.css" rel="stylesheet">
  <link href="assets/hover.min.css" rel="stylesheet">

  <!-- Blog Stylesheet File -->
  <link href="assets/blog.css" rel="stylesheet">

  <!-- Main Stylesheet File -->
  <link href="assets/style.css" rel="stylesheet">

  <!-- Responsive css -->
  <link href="assets/responsive.css" rel="stylesheet">

  <!-- Favicon -->
  <link rel="shortcut icon" href="assets/favicon.png">

  <!-- =======================================================
    Theme Name: Folio
    Theme URL: https://bootstrapmade.com/folio-bootstrap-portfolio-template/
    Author: BootstrapMade.com
    Author URL: https://bootstrapmade.com
  ======================================================= -->
</head>

<body>

  <!-- start section navbar -->
  <nav id="main-nav-subpage" class="subpage-nav">
    <div class="row">
      <div class="container">

        <div class="logo">
          <a href="/index"><img src="assets/logo.jpg" alt="logo"></a>
        </div>

        <div class="responsive"><i data-icon="m" class="ion-navicon-round"></i></div>

       <ul class="nav-menu list-unstyled">
          <li><a href="#header" class="smoothScroll">Home</a></li>
          <li><a href="#about" class="smoothScroll">About</a></li>
          <li><a href="#journal" class="smoothScroll">Recruiting</a></li>
          <li><a href="#portfolio" class="smoothScroll">Contest</a></li>
          <li><a href="#portfolio2" class="smoothScroll">Participation</a></li>
          <li><a href="#contact" class="smoothScroll">Contact</a></li>
          <%if user_signed_in?%>
          <%=current_user.name%>
          <%end%>
          </li>
        </ul>
      </div>
    </div>
  </nav>
  <!-- End section navbar -->
  <!-- start section main content -->
  <div class="main-content paddsection">
    <div class="container">
      <div class="row justify-content-center">
        <div class="col-md-8 col-md-offset-2">
          <div class="row">
            <div class="container-main single-main">
              <div class="col-md-12">
                <div class="block-main mb-30">
                  
                  <div class="content-main single-post padDiv">
                    <div class="journal-txt">
                    <h1>Photographer</h1>
                     <br>
                     <%= link_to 'New Post', new_postnew3_path %><br>
                     <br>
                    </div>
                    <div class="post-meta">
                          <table class="table table-hover">
                             <thead>
                              <tr>
                              <th>Title</th>
                              <th>User</th>
                              <th colspan="3"></th>
                             </tr>
                            </thead>
                            <tbody>
                             <% @postnew3s.each do |postnew3| %>
                             <tr>
                             <td><%= postnew3.title %></td>
                             <td><%= postnew3.user.email %></td>
                             <td><%= link_to 'Show', postnew3 %></td>
                             <td>
                                <%= link_to 'Edit', edit_postnew3_path(postnew3) %>
                             </td>
                             <td>
                                <%= link_to 'Destroy', postnew3, method: :delete, data: { confirm: 'Are you sure?' } %>
                             </td>
                             </tr>
                             <% end %>
                           </tbody>
                          </table><br>
                         
                      </ul>
                    </div>
                    
                  </div>
                </div>
              </div>
            
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!--  </div> -->
  <!-- start section main content -->

  

  <!-- JavaScript Libraries -->
  <script src="assets/jquery.min.js"></script>
  <script src="assets/jquery-migrate.min.js"></script>
  <script src="assets/bootstrap.bundle.min.js"></script>
  <script src="assets/typed.js"></script>
  <script src="assets/owl.carousel.min.js"></script>
  <script src="assets/magnific-popup.min.js"></script>
  <script src="assets/isotope.pkgd.min.js"></script>

  <!-- Contact Form JavaScript File -->
  <script src="assets/contactform.js"></script>

  <!-- Template Main Javascript File -->
  <script src="assets/main.js"></script>

</body>

</html>



